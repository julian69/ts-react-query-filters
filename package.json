{
  "name": "qonto-test-playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "cy:run-e2e": "cypress run --e2e --reporter mochawesome",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:withServer": "start-server-and-test dev http-get://localhost:3000 cy:open-e2e"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.15.3",
    "@tanstack/react-query-devtools": "^5.17.21",
    "install": "^0.13.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.14.6",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "mochawesome": "^7.1.3",
    "postcss": "^8.4.33",
    "react-loader-spinner": "^6.1.6",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  }
}
